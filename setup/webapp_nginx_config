# webapp portion
# This is a config file for NGINX
# In essence, this file points NGINX to the sock file generated by uWsgi
# then takes said sock file, and generates an http client to be hosted on
# the world wide web

server {
    # if you have a domain
    # server_name your_domain www.your_domain;

    # else
    # IP address

    location / {
        # specifies some general uWSGI parameters
        include uwsgi_params;

        uwsgi_pass unix:/home/swalt/git/cosc310_l09_proj/webapp.sock;
    }


}