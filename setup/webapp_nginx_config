# webapp portion
# This is a config file for NGINX
# In essence, this file points NGINX to the sock file generated by uWsgi
# then takes said sock file, and generates an http client to be hosted on
# the world wide web

server {

        root /home/swalt/git/cosc310_l09_proj/WebApp/build;
        index index.html index.htm index.nginx-debian.html;

        server_name wss-cosc310.ddns.net;

        location / {
                try_files $uri $uri/ =404;
        }
}